<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_for_correct_rating</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Rating__c &lt; 1,  Rating__c &gt; 5)</errorConditionFormula>
    <errorDisplayField>Rating__c</errorDisplayField>
    <errorMessage>Please enter value from 1 to 5</errorMessage>
</ValidationRule>
